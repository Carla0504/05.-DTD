<!DOCTYPE liga [
    <!ELEMENT liga (jugador+)>
    <!ELEMENT jugador (nombre, apellidos, equipo, edad)>
    <!ELEMENT nombre (#PCDATA)>
    <!ELEMENT apellidos (#PCDATA)>
    <!ELEMENT equipo (ciudad, presidente)>
